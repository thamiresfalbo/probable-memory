<%= form_with url: login_path, method: :post do |f| %>
    <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
    </div>

    <div class="field">
        <%= f.label :pasword %><br />
        <%= f.password_field :pasword %>
    </div>

    <div class="actions">
        <%= f.submit "Login" %>
    </div>
<% end %>
